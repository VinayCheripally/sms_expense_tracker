{
  "expo": {
    "name": "SmartExpense",
    "slug": "smart-expense-tracker",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "smartexpense",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "dev.expo.expensetracker",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECEIVE_SMS",
        "android.permission.READ_SMS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "df62a7d3-37eb-45ca-b307-a6f6182cdd32"
      }
    }
  }
}